/* pages/select_city/select_city.wxss */

page {
     width: 100%;
     height: 100%;
     background: white;
}

.cityListLetter {
     width: 100%;
     padding: 16rpx 24rpx 0rpx;
     font-size: 30rpx;
     font-weight: bold;
     color: #ff7676;
     /* background-color: skyblue; */
}

.cityListItem {
     width: 100%;
     box-sizing: border-box;
     padding: 20rpx 10rpx 10rpx;
     margin-left: 14rpx;
     font-size: 26rpx;
     background-color: white;
     border-bottom: #dedede 1rpx solid;
}
